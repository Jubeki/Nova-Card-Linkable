{
    "name": "jubeki/nova-card-linkable",
    "description": "A Laravel Nova card, which has a link",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "laravel/nova": "^4.0",
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Jubeki\\Nova\\Cards\\Linkable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jubeki\\Nova\\Cards\\Linkable\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
